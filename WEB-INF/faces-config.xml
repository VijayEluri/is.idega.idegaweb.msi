<?xml version="1.0"?>
<faces-config
	xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
version="1.2">

	<managed-bean>
		<managed-bean-name>raceBusiness</managed-bean-name>
		<managed-bean-class>is.idega.idegaweb.msi.business.RaceBusiness</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>modificationPeriodBean</managed-bean-name>
		<managed-bean-class>is.idega.idegaweb.msi.presentation.bean.data.ModificationPeriodBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>modificationPeriodTypeBean</managed-bean-name>
		<managed-bean-class>is.idega.idegaweb.msi.presentation.bean.data.ModificationPeriodTypeBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modificationPeriodTypesBean</managed-bean-name>
		<managed-bean-class>is.idega.idegaweb.msi.presentation.bean.service.ModificationPeriodTypesBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>modificationPeriodsBean</managed-bean-name>
		<managed-bean-class>is.idega.idegaweb.msi.presentation.bean.service.ModificationPeriodsBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

	<component>
		<component-type>Modification period editor</component-type>
		<component-class>is.idega.idegaweb.msi.presentation.ModificationPeriodEditor</component-class>
		<component-extension>
			<idegaweb-info>
        		<builder-visible>true</builder-visible>
        		<object-type>jsf.uicomponent</object-type>
      		</idegaweb-info>
		</component-extension>
	</component>

	<component>
		<component-type>Modification periods viewer</component-type>
		<component-class>is.idega.idegaweb.msi.presentation.ModificationPeriodsViewer</component-class>
		<component-extension>
			<idegaweb-info>
        		<builder-visible>true</builder-visible>
        		<object-type>jsf.uicomponent</object-type>
      		</idegaweb-info>
		</component-extension>
	</component>

	<component>
		<component-type>Modification period type editor</component-type>
		<component-class>is.idega.idegaweb.msi.presentation.ModificationPeriodTypeEditor</component-class>
		<component-extension>
			<idegaweb-info>
        		<builder-visible>true</builder-visible>
        		<object-type>jsf.uicomponent</object-type>
      		</idegaweb-info>
		</component-extension>
	</component>

	<component>
		<component-type>Modification period types viewer</component-type>
		<component-class>is.idega.idegaweb.msi.presentation.ModificationPeriodTypesViewer</component-class>
		<component-extension>
			<idegaweb-info>
        		<builder-visible>true</builder-visible>
        		<object-type>jsf.uicomponent</object-type>
      		</idegaweb-info>
		</component-extension>
	</component>

</faces-config>
